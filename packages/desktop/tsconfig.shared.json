{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022"],
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": false,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "outDir": "dist/shared/excel",
    "rootDir": "src/shared/excel",
    "baseUrl": "."
  },
  "include": ["src/shared/excel/**/*"],
  "exclude": ["node_modules", "dist"]
}
